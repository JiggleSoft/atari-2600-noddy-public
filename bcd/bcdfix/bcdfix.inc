;------------------------------------------------------------------------------
; DECIMAL MODE
;------------------------------------------------------------------------------

;------------------------------------------------------------------------------
; Save current decimal mode on the stack and enable decimal mode (inline).
;
; Input:    None
; Working:  SR->STACK
; Output:   D=1
; Statistics:-
;   Code:   2
;   Stack:  1 (pushed)
;   Cycles: 5 (3+2)
;------------------------------------------------------------------------------
    .MACRO  BCDFIX_MODE_INIT
            PHP
            SED
    .ENDMACRO
;------------------------------------------------------------------------------



;------------------------------------------------------------------------------
; Restore previously saved decimal mode from the stack (inline).
;
; Input:    None
; Working:  STACK->SR
; Output:   SR=Restored SR.
; Statistics:-
;   Code:   1
;   Stack:  1 (popped)
;   Cycles: 4
;------------------------------------------------------------------------------
.MACRO      BCDFIX_MODE_DONE
            PLP
.ENDMACRO
